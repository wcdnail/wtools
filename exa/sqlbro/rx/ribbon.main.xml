        <Command Name="cmdExit" Symbol="RIDC_EXIT" LabelTitle="E&amp;xit" />
        <Command Name="cmdMRUList" LabelTitle="..." />
        <Command Name="tabOne" LabelTitle="#1" />
        <Command Name="tabTwo" LabelTitle="#2" />
    </Application.Commands>
    <Application.Views>
